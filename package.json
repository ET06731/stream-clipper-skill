  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ET06731/stream-clipper-skill.git"
  },
  "bugs": {
    "url": "https://github.com/ET06731/stream-clipper-skill/issues"
  },
  "homepage": "https://github.com/ET06731/stream-clipper-skill#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "commander": "^9.0.0",
    "chalk": "^4.1.2"
  }
}
