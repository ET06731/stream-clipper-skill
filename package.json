{
  "name": "stream-clipper-cli",
  "version": "1.0.0",
  "description": "Bilibili直播切片智能剪辑工具 - 支持字幕提取、弹幕分析、智能切片",
  "main": "index.js",
  "bin": {
    "stream-clipper": "./bin/cli.js",
    "clipper": "./bin/cli.js"
  },
  "scripts": {
    "postinstall": "node scripts/setup.js",
    "download": "node bin/cli.js download",
    "analyze": "node bin/cli.js analyze",
    "clip": "node bin/cli.js clip",
    "upload": "node bin/cli.js upload",
    "record": "python scripts/record_workflow.py",
    "extract-subtitles": "python scripts/extract_subtitles.py",
    "generate-title": "python scripts/generate_title_from_subtitle.py"
  },
  "keywords": [
    "bilibili",
    "stream",
    "clipper",
    "video",
    "danmaku",
    "subtitle",
    "whisper",
    "vtuber",
    "直播切片"
  ],
